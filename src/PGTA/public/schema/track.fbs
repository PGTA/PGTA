file_identifier "PGTA";
namespace PGTASchema;

table UUID
{
    uuid:[byte];
}

table Sample
{
    name:string;
    defaultFile:string;
    startTime:long = 0;
    frequency:long;
    probability:float;
    volumeMultiplier:float = 1;
    groupIds:[UUID];

}

table Group
{
    groupName:string;
    uuid:UUID;
}

table Track
{
    samples:[Sample];
}

root_type Track;
